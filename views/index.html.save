<body id="top">
  <header>
    {loop="$popups"} {if="$value.popup_active == $displayPopupControl"}
    <div
      class="modal fade"
      id="{$key}"
      tabindex="-1"
      role="dialog"
      aria-labelledby="myModalLabel"
      aria-hidden="true"
    >
      <!-- Change class .modal-sm to change the size of the modal -->
      <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h2 class="modal-title w-100" id="myModalLabel">
              {$value.popup_title}
            </h2>
            <button
              type="button"
              id="btn-{$key}"
              class="close"
              data-dismiss="modal"
              aria-label="Close"
            >
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <img src="{$value.desphoto}" alt="popups" class="img-fluid" />
          </div>
        </div>
      </div>
    </div>
    {else} {$displayPopupControl = 1} {/if} {/loop}
